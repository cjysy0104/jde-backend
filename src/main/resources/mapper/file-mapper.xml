<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.jde.file.dao.FileMapper">
	
	<select id="getDefaultImage" resultType="String">
		SELECT 
		       FILE_URL
		  FROM 
		       TB_DEFAULT_PROFILE
	</select>
	
	<select id="getfileUrl" resultType="String">
		SELECT
		       FILE_URL fileUrl
		  FROM
		       TB_MEMBER_FILE
		 WHERE
		       MEMBER_NO = #{memberNo}
	</select>
	
	
</mapper>